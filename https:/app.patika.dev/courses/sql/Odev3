1.SORU CEVAPLARI

SELECT * FROM country
WHERE country LIKE 'A%a';

2.SORU CEVAPLARI

SELECT * FROM country
WHERE country LIKE '_____%n'

3.SORU CEVAPLARI

SELECT * FROM film 
WHERE title ~~* '%t%t%t%t%'

4.SORU CEVAPLARI

SELECT * FROM film
WHERE title LIKE 'C%' AND length >90 AND rental_rate =2.99;
